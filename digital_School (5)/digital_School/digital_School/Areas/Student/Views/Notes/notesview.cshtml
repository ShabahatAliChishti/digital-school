@model IEnumerable<digital_School.Models.Lecture>
<!DOCTYPE html>
<html lang="en">

<head>
    @{
        ViewBag.Title = "notes";
    }

</head>
<body id="page-top">




    <!-- Page Wrapper -->
    <div id="wrapper">


        <!-- End of Topbar -->
        <!-- Begin Page Content -->
        <div class="container" style="background-color:white ; width:80% ;margin-top:50px;margin-right:80px;">

            <!-- Page Heading -->
            <!-- DataTales Example -->
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">School</h6>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="MyTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Lecture</th>
                                    <th>Date</th>
                                    <th>View</th>
                                    <th>Description</th>
                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            <a href="~/file_upload/@item.LectureUrl" />
                                            @Html.DisplayFor(modelitem => item.LectureUrl)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelitem => item.CreatedDate)
                                        </td>
                                        <td>
                                            <iframe width="530" height="400" src="@item.VideoLink" frameborder="0" allowfullscreen></iframe>
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelitem => item.Lecture_Description)
                                        </td>
                                    </tr>

                                }

                            </tbody>
                        </table>








                    </div>
                </div>

            </div>

        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- End of Main Content -->
    <!-- Footer -->
    <!-- End of Footer -->
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>



</body>


</html>*
